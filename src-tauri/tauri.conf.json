{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "lme-trade-request",
  "version": "0.1.0",
  "identifier": "com.example.lmetrade",
  "build": {
    "frontendDist": "../dist",
    "beforeDevCommand": "",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "lme-trade-request",
        "width": 1040,
        "height": 1240,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
   },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
